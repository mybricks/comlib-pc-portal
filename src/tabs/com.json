{
  "title": "门户Tabs",
  "enable": true,
  "namespace": "fangzhou.portal-pc.tabs",
  "author": "ZhangZhenfeng",
  "author_name": "张振丰",
  "version": "1.0.8",
  "description": "门户Tabs组件",
  "icon": "https://ali2.a.kwimgs.com/udata/pkg/ks-merchant/fangzhou/static_picture/tab.2b7188c7f5378c24.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "docs": "https://docs.corp.kuaishou.com/k/home/VSNdpUEu77JQ/fcAASn_JyoIBPcizB5CCQRb4c",
  "slots": [
    {
      "id": "tab0",
      "title": "标签页内容"
    }
  ],
  "inputs": [
    {
      "id": "previous",
      "title": "上一页",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "next",
      "title": "下一页",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "active",
      "title": "激活tab项",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "outActiveTab",
      "title": "获取激活tab项",
      "rels": ["outActiveTab"],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "tabClick",
      "title": "标签页点击",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "outActiveTab",
      "title": "激活tab项输出",
      "schema": {
        "type": "string"
      }
    }
  ]
}
